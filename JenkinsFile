node {
   stage('checkout') {
      git 'https://github.com/SereginPavel/EasyMoney.git'
    }

    stage('build'){
        bat 'mvn verify'
    }

    stage('install'){
        copy C:\Program Files (x86)\Jenkins\workspace\pipeline-jenkins-example\target\EasyMoney-1.0.war   C:\Users\SerP\tomcat\webapps\EasyMoney-1.0.war
    }

 }