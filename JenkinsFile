node {
   stage('checkout') {
      git 'https://github.com/SereginPavel/EasyMoney.git'
    }

    stage('build'){
        bat 'mvn verify'
    }

    stage('install'){
        steps {
                fileOperations {
                    fileCopyOperation('C:\\file2.txt', '', 'D:\\.', false)
                }
    }

 }
 }