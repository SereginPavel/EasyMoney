node {
   stage('checkout') {
      git 'https://github.com/SereginPavel/EasyMoney.git'
    }

    stage('build'){
        bat 'mvn verify'
    }

     stage('Install') {
        bat 'copy /Y target\\EasyMoney-1.0.war C:\\Users\\SerP\\tomcat\\webapps'
        bat 'C:\\Users\\SerP\\tomcat\\bin\\startup.bat'
 }
 }